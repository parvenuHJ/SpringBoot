		<tbody>
			<!-- th~ : 타임리프 문법
			each - 포이치문
			text - 해당 내용을 태그안에 출력
				
				
			 -->
			<tr th:each="board,loop:${boardList}">
				<td th:text="${loop.count}"></td>
				<td th:text="${board.boardTitle}"></td>
				<td th:text="${board.boardDate}"></td>
			</tr>
		</tbody>
	</table>
	

